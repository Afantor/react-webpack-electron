{
  "name": "react-webpack-electron",
  "version": "0.0.1",
  "description": "start-up demo for Electron App using React and Webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack -p --env production && electron .",
    "dev-server": "webpack-dev-server",
    "electron": "electron .",
    "build:dev":"webpack",
